  <br>
  <script>
    (() => {
      const params = getParam()
      const isDev = params.includes("dev");
      if (isDev) { $("#rightbar-exitdev").show() }
      $("#rightbar-theme").on("click", () => changeTheme());
      $("#rightbar-translate").on("click", () => changeLanguage());
      $("#rightbar-top").on("click", () => BODY.scrollTop(0));
      $("#rightbar-github").on("click", () => open("https://github.com/manmen2414/manmen2414.github.io", "_blank"))
      $("#rightbar-exitdev").on("click", () => setParam(params.filter(v => v !== "dev"), true))
    })()
  </script>
