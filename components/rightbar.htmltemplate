<div id="rightbar">
  <style>
    #rightbar {
      background-color: aliceblue;
      position: fixed;
      text-align: left;
      top: 20px;
      right: -190px;
      width: 255px;
      padding-bottom: 10px;
      border-radius: 20px 0 0 20px;
      z-index: 10;
      transition: right 0.2s ease-in-out;
    }

    #rightbar:hover {
      right: 0px;
    }

    #rightbar * {
      color: #000;
    }

    #rightbar button {
      background-color: transparent;
      border: none;
      cursor: pointer;
      margin: 10px 0px 0px 10px;
      white-space: nowrap;
    }

    #rightbar button>span {
      font-size: 25px;
      line-height: 28px;
      padding-right: 100px;
    }
  </style>
  <div id="rightbar-space" style="margin: 10px;"></div>
